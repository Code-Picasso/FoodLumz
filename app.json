{
  "expo": {
    "name": "FoodLums",
    "slug": "FoodLums",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/imgs/spalsh-img.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/imgs/spalsh-img.png",
      "resizeMode": "contain",
      "backgroundColor": "#D3D3D3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "softwareKeyboardLayoutMode": "pan"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/imgs/spalsh-img.png",
        "backgroundColor": "#D3D3D3"
      },
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustResize"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
